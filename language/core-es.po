msgid "Encrypted CC (last four: %s)"
msgstr "CC cifrada (las cuatro últimas: %s)"

msgid "Encryption Key"
msgstr "Clave de cifrado"

msgid "Encryption is recommended for your data security"
msgstr "Se recomienda el cifrado para la seguridad de sus datos"

msgid "Encryption password"
msgstr "Contraseña de cifrado"

msgid "End"
msgstr "Fin"

msgid "End Date"
msgstr "Fecha final"

msgid "End of Billing Cycle"
msgstr "Fin del ciclo de facturación"

msgid "End of Billing Period"
msgstr "Fin del período de facturación"

msgid "End-user license agreement"
msgstr "Contrato de licencia para el usuario final"

